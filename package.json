{
  "name": "liftbuddy-server",
  "version": "1.0.0",
  "description": "server for liftbuddy",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node.js",
    "startt": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd ../LiftBuddy-Client && npm run start",
    "build": "cd client && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DanielSochor/LiftBuddy-Server.git"
  },
  "author": "daniel-sochor",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DanielSochor/LiftBuddy-Server/issues"
  },
  "homepage": "https://github.com/DanielSochor/LiftBuddy-Server#readme",
  "dependencies": {
    "concurrently": "^4.1.2",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "nodemon": "^1.19.2"
  }
}
